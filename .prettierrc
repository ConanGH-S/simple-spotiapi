{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "none",
	"printWidth": 800,
	"semi": false,
	"bracketSpacing": true,
	"arrowParens": "always",
	"htmlWhitespaceSensitivity": "css",
	"tabWidth": 2,
	"plugins": ["prettier-plugin-svelte"],
	"pluginSearchDirs": ["."],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
